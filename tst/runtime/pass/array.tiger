let
	var N := 8
	type intArray = array of int
	var row := intArray [5] of 3
	var col := intArray [N + N - 1] of 4
in
	row [0] := 5;
	printi (row [0]);
	print("\n");
	row [1] := 5 + 1;
	printi (row [1]);
	print("\n");
	row [2] := 5 + row [1];
	printi (row [2]);
	print("\n");
	row [3] := 5 + col [1];
	printi (row [3]);
	print("\n");
	row [4] := 5 + col [0];
	printi (row [4])
end
