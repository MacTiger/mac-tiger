EXIT_EXC EQU 64
READ_EXC EQU 65
WRITE_EXC EQU 66
STACK_ADRS EQU 0X1000
LOAD_ADRS EQU 0xF000
NIL EQU 0

SP EQU R15
WR EQU R14
BP EQU R13

ORG LOAD_ADRS
START main_

LDW SP, #STACK_ADRS
STRING0 string "Hello World !"

main_
	LDW R1, #STRING0
	STW R1, (-SP) //met le string en param de fonction appel√©e

	TRP #EXIT_EXC
