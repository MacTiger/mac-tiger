EXIT_EXC EQU 64
READ_EXC EQU 65
WRITE_EXC EQU 66
STACK_ADRS EQU 0X1000
LOAD_ADRS EQU 0xF000
NIL EQU 0

SP EQU R15
WR EQU R14
BP EQU R13

ORG LOAD_ADRS
START main_

LDW SP, #STACK_ADRS

mult_
	LDW R1, (SP)2
	LDW R2, (SP)4
	MUL R1, R2, R0
	RTS

main_
	LDW SP, #STACK_ADRS
	LDW R3, #3
	LDW R4, #4
	STW R3, -(SP)
	STW R4, -(SP)
	JSR @mult_
